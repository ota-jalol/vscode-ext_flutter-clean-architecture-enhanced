{
  "name": "flutter-clean-architecture-enhanced",
  "displayName": "Flutter Clean Architecture - Enhanced",
  "description": "Enhanced version: Complete Clean Architecture with Freezed, modern BLoC, Dio, Dartz and 15+ generated files per feature",
  "version": "3.1.1",
  "publisher": "OtaJalol",
  "repository": {
    "type": "git",
    "url": "https://github.com/ota-jalol/vscode-ext_flutter-clean-architecture-enhanced.git"
  },
  "license": "MIT",
  "author": {
    "name": "OtaJalol",
    "email": "otajalol@example.com"
  },
  "contributors": [
    {
      "name": "KiritchoukC",
      "url": "https://github.com/KiritchoukC"
    }
  ],
  "engines": {
    "vscode": "^1.54.0"
  },
  "homepage": "hhttps://github.com/ota-jalol/vscode-ext_flutter-clean-architecture-enhanced",
  "bugs": {
    "url": "https://github.com/ota-jalol/vscode-ext_flutter-clean-architecture-enhanced/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "clean-architecture",
    "dart",
    "flutter",
    "bloc",
    "state-management",
    "freezed",
    "dio",
    "dartz",
    "getit",
    "enhanced"
  ],
  "icon": "assets/logo.png",
  "activationEvents": [
    "onCommand:extension.new-feature",
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.new-feature-bloc",
        "title": "New Feature",
        "category": "Flutter Clean Architecture",
        "icon": "assets/logo.png"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.new-feature-bloc",
          "group": "cleanArchitectureGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "deploy": "vsce publish",
    "package-vsix":" npm run package"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.17.16",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.7.8",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^6.0.2",
    "cz-conventional-changelog": "^3.0.2",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^5.8.3",
    "vsce": "^1.81.1",
    "vscode": "^1.1.37",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.21",
    "mkdirp": "^0.5.1",
    "semver": "^6.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}